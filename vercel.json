{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://server-fr8ahc1rq-bc-82a48503.vercel.app/api/$1"
    },
    {
      "source": "/uploads/(.*)",
      "destination": "https://server-fr8ahc1rq-bc-82a48503.vercel.app/uploads/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

